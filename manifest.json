{
  "manifest_version": 2,
  "name": "nsScript",
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": ["https://nutisport.eu/"]
  },
  "version": "3.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/p5@1.4.1/lib/p5.min.js; object-src 'self'",
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "sketch/index.html",
    "default_title": "nsScript"
  }
}
